  "scripts": {
    "projects:install": "npm-run-all --serial install:*",
    "projects:start": "npm-run-all --parallel start:*",
    "install:main": "cd projects/main && yarn",
    "start:main": "cd projects/main && yarn start",
    "start-main-multiple": "cd projects/main && yarn start:multiple",
    "install:react16": "cd projects/react16 && yarn",
    "start:react16": "cd projects/react16 && yarn start",
    "install:react15": "cd projects/react15 && yarn",
    "start:react15": "cd projects/react15 && yarn start",
    "install:vue": "cd projects/vue && yarn",
    "start:vue": "cd projects/vue && yarn start",
    "install:angular9": "cd projects/angular9 && yarn",
    "start:angular9": "cd projects/angular9 && yarn serve:qiankun",
    "install:purehtml": "cd projects/purehtml && yarn",
    "start:purehtml": "cd projects/purehtml && yarn start",
    "install:vue3": "cd projects/vue3 && yarn",
    "start:vue3": "cd projects/vue3 && yarn start"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "qiankun": "^2.10.16"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  }
